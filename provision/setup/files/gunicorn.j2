description "Gunicorn application server handling {{ project_name }}"

[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=vagrant
Group=www-data
WorkingDirectory={{ project_path }}/backend
#ExecStart=/usr/bin/gunicorn --bind unix:{{ project_path }}/{{project_name}}.sock {{ project_name }}.wsgi:application
ExecStart=/usr/bin/gunicorn --bind 0.0.0.0:8000 {{ project_name }}.wsgi:application

[Install]
WantedBy=multi-user.target